{
  "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "swagger-gen/api-docs.json",
  "output": "src/app/api",
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "templates": "swagger-gen/templates"
}